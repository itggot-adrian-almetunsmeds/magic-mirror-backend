
var scheme = "<%= @scheme %>";
var uri = scheme + window.document.location.host + "/";
var ws = new WebSocket(uri);

ws.onmessage = function (message) {
    console.log(JSON.parse(message.data))
}

ws.onopen = function () {
    ws.send('Connected to ws')
}
ws.onclose = function () {
    console.log('Disconnected from ws')
}
